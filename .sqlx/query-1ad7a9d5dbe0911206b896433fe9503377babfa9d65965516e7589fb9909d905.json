  "query": "SELECT DISTINCT l.line_cd,\n            l.line_name,\n            l.line_name_k,\n            l.line_name_h,\n            l.line_name_r,\n            l.line_name_zh,\n            l.line_name_ko,\n            l.line_color_c,\n            l.company_cd,\n            l.line_type,\n            l.line_symbol1,\n            l.line_symbol2,\n            l.line_symbol3,\n            l.line_symbol4,\n            l.line_symbol1_color,\n            l.line_symbol2_color,\n            l.line_symbol3_color,\n            l.line_symbol4_color,\n            l.line_symbol1_shape,\n            l.line_symbol2_shape,\n            l.line_symbol3_shape,\n            l.line_symbol4_shape,\n            l.e_status,\n            l.e_sort,\n            l.average_distance,\n            sst.line_group_cd,\n            s.station_cd,\n            s.station_g_cd\n        FROM `lines` AS l\n        JOIN `stations` AS s ON s.station_g_cd = ?\n            AND s.e_status = 0\n        JOIN `station_station_types` AS sst ON sst.station_cd = s.station_cd AND sst.pass <> 1\n        WHERE l.line_cd = s.line_cd\n            AND l.e_status = 0",
