# Test configuration
# This file should be sourced before running integration tests
# Usage: source .env.test && cargo test --features integration-tests

export TEST_DATABASE_URL=postgres://test:test@localhost/stationapi_test

# Rust specific test environment
export RUST_LOG=debug
export RUST_BACKTRACE=1

# Test behavior flags
export RUST_TEST_THREADS=1  # Run tests sequentially to avoid database conflicts
